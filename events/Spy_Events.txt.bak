country_event = { # Расчет вероятности похищения (бонусы)
	id = spy.6
	title = spy.6.t
	desc = spy.6.d
	picture = GFX_report_event_diplomatic_message
	is_triggered_only = yes
    hidden = yes

	option = {
		name = spy.6.a
		if = {
		limit = {NOT = {FROM = {has_idea = SPY_BONUS}}}
		if = {
		limit = {ROOT = {NOT = {has_idea = SPYDEF_BONUS}}}
		FROM = {
		set_temp_variable = {temp1 = StealPower}
		set_temp_variable = {temp2 = PREV.DefendPower}
		subtract_from_temp_variable = {temp1 = temp2}
		set_variable = {StealChance = temp1}
		}
		FROM = {
		set_temp_variable = {temp1 = StealPower}
		set_temp_variable = {temp2 = PREV.DefendPower}
		set_temp_variable = {temp3 = 100}
		subtract_from_temp_variable = {temp1 = temp2}
		subtract_from_temp_variable = {temp3 = temp1}
		set_variable = {StealUnchance = temp3}
		}
		else = {
		FROM = {
		set_temp_variable = {temp1 = StealPower}
		set_temp_variable = {temp2 = PREV.DefendPower}
		add_to_temp_variable = {temp2 = 15}
		subtract_from_temp_variable = {temp1 = temp2}
		set_variable = {StealChance = temp1}
		}
		FROM = {
